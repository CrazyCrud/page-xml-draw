                {
                    "$ref": "#/Drawing/WebColor"
                }
            ]
        },
        "FillOpacity": {
            "description": "Opacity to fill the polygons with",
            "type": "number",
            "minimum": 0,
            "maximum": 1
        },
        "StrokeColor": {
